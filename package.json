    "expo-font": "~10.0.3",
    "expo-image-manipulator": "~10.1.2",
    "expo-image-picker": "~11.0.3",
    "expo-linear-gradient": "~10.0.3",
    "expo-location": "~13.0.4",
    "expo-random": "~12.0.1",
    "expo-secure-store": "~11.0.3",
    "expo-status-bar": "~1.1.0",
    "expo-web-browser": "~10.0.3",
    "faker": "^5.5.3",
